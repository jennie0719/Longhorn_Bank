@model IEnumerable<String>

<h1>Error</h1>

<p>Oops - something went wrong.</p>

@if (Model != null)
{
    foreach (String message in Model)
    {
        <p><span class="text-danger">@message</span></p>
    }
}

<!--TODO: change this!!!!-->
@if (User.IsInRole("Customer"))
{
    <a asp-action="Index" asp-controller="Accounts" class="btn btn-success">Take me home!</a>
}
else
{
    <a asp-action="StaffHome" asp-controller="Home" class="btn btn-success">Take me home!</a>
}

